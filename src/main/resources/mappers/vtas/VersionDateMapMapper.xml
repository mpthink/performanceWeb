<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dell.petshow.vtas.mapper.VersionDateMapMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.dell.petshow.vtas.entity.VersionDateMap">
		<id column="VERSION" property="version" />
		<result column="DATE" property="date" />
		<result column="PROGRAM" property="program" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        VERSION AS version, DATE AS date, PROGRAM AS program
    </sql>
    
    <select id="selectByVersion" resultType="com.dell.petshow.vtas.entity.VersionDateMap" resultMap="BaseResultMap">
    	SELECT * 
    	FROM version_date_map 
    	where version=#{version}
    </select>
</mapper>
